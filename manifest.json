{
  "manifest_version": 2,
  "name": "Oldtimer",
  "version": "1.0.0",

  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "/content_script.js" ]
    }
  ],

  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "/palette.svg"
    },
    "default_title": "Oldtimer",
    "default_popup": "/popup.html"
  },

  "permissions": [ "storage" ]
}
